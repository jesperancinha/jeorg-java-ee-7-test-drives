<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <enterprise-beans>
        <session>
            <ejb-name>ListOfKingsAndQueensBean</ejb-name>
            <ejb-class>org.jesperancinha.jtd.jee.the.netherlands.servlet.ListOfKingsAndQueensBean</ejb-class>
        </session>
        <session>
            <ejb-name>ListOfKingsAndQueensBeanForOrangeNassau</ejb-name>
            <ejb-class>org.jesperancinha.jtd.jee.the.netherlands.servlet.ListOfKingsAndQueensBeanForOrangeNassau
            </ejb-class>
        </session>
    </enterprise-beans>
    <assembly-descriptor>
        <security-role>
            <role-name>Manager</role-name>
        </security-role>
        <security-role>
            <role-name>Civilian</role-name>
        </security-role>
        <security-role>
            <role-name>OranjeNassau</role-name>
        </security-role>
        <method-permission>
            <role-name>Manager</role-name>
            <role-name>Civilian</role-name>
            <method>
                <ejb-name>ListOfKingsAndQueensBean</ejb-name>
                <method-name>*</method-name>
            </method>
        </method-permission>
        <method-permission>
            <role-name>OranjeNassau</role-name>
            <method>
                <ejb-name>ListOfKingsAndQueensBeanForOrangeNassau</ejb-name>
                <method-name>*</method-name>
            </method>
        </method-permission>
    </assembly-descriptor>
</ejb-jar>